SELECT AVG(waga), rodzaj FROM kreatura WHERE rodzaj = 'wiking';
SELECT AVG(waga), COUNT(idKreatury), rodzaj FROM kreatura GROUP BY rodzaj;
SELECT ROUND(2022 - AVG(YEAR(dataUr))) AS 'srednia wieku', rodzaj FROM kreatura GROUP BY rodzaj;
// SELECT AVG(YEAR(CURDATE()) - YEAR(dataUr)) AS 'sredni wiek', rodzaj FROM kreatura GROUP BY rodzaj;     (inna wersja)

SELECT SUM(waga * ilosc), rodzaj FROM zasob GROUP BY rodzaj;
SELECT AVG(waga), nazwa FROM zasob WHERE ilosc >= 4 GROUP BY nazwa HAVING SUM(waga)>10;     // glupi podpunkt, nieprecyzyjne polecenie
SELECT COUNT(DISTINCT nazwa), rodzaj FROM zasob WHERE ilosc > 1 GROUP BY rodzaj;

SELECT kreatura.nazwa, SUM(ekwipunek.ilosc) FROM kreatura JOIN ekwipunek ON kreatura.idKreatury = ekwipunek.idKreatury GROUP BY nazwa;
SELECT kreatura.nazwa, GROUP_CONCAT(DISTINCT zasob.nazwa) AS zasoby FROM kreatura JOIN ekwipunek ON kreatura.idKreatury = ekwipunek.idKreatury
JOIN zasob ON zasob.idZasobu = ekwipunek.idZasobu GROUP BY kreatura.nazwa;
SELECT kreatura.nazwa FROM kreatura LEFT JOIN ekwipunek ON ekwipunek.idKreatury = kreatura.idKreatury WHERE ekwipunek.idKreatury IS NULL;

SELECT kreatura.nazwa, GROUP_CONCAT(DISTINCT zasob.nazwa) AS zasoby FROM kreatura JOIN ekwipunek ON kreatura.idKreatury = ekwipunek.idKreatury 
JOIN zasob ON zasob.idZasobu = ekwipunek.idZasobu WHERE kreatura.rodzaj = 'wiking' AND YEAR(dataUr) BETWEEN 1670 AND 1679 GROUP BY kreatura.nazwa;
// podobne do 3.2
SELECT DISTINCT kreatura.nazwa FROM kreatura JOIN ekwipunek ON kreatura.idKreatury = ekwipunek.idKreatury
JOIN zasob ON zasob.idZasobu = ekwipunek.idZasobu WHERE zasob.rodzaj = 'jedzenie' ORDER BY dataUr DESC LIMIT 5;
SELECT A.nazwa AS kreatura1, B.nazwa AS kreatura2 FROM kreatura A, kreatura B WHERE A.idKreatury <> B.idKreatury AND A.idKreatury + 5 = B.idKreatury;

SELECT kreatura.nazwa, ROUND(AVG(zasob.waga), 2) AS 'srednia waga' FROM kreatura JOIN ekwipunek ON kreatura.idKreatury = ekwipunek.idKreatury
JOIN zasob ON zasob.idZasobu = ekwipunek.idZasobu WHERE kreatura.rodzaj != 'waz' AND kreatura.rodzaj != 'malpa'
GROUP BY kreatura.nazwa HAVING SUM(zasob.ilosc) < 30;
SELECT GROUP_CONCAT(nazwa, ' ', dataUr), rodzaj FROM kreatura GROUP BY rodzaj;      // nwm jesszcze jak zrobic
